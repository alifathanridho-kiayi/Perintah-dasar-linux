#membuat laporan statistik output ke file report text menggunakan perintah ls,wc,du,dan piping
alifathan@alifathan-VirtualBox:~$ {
echo "=== LAPORAN STATISTIK FILE SISTEM ==="
echo
echo "[1] Daftar File dan Folder:"
ls -l
echo
echo "[2] Jumlah File & Folder: "
ls | wc -l
echo
echo "[3] Total Ukuran DirektORI:" du -sh
}> report.txt
alifathan@alifathan-VirtualBox: $ cat eport text
cat: eport: No such file or directory 
cat: text: No such file or directory 
alifathan@alifathan-VirtualBox:~$ cat report.txt 
