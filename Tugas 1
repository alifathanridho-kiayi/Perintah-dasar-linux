#perintah find mv dan cp
alifathan@alifathan-VirtualBox: $ find. -type f -name "*.txt" -exec mv {} documents/ \; 
mv: './documents/first-index.txt' and 'documents/first-index.txt' are the same file mv: './documents/last-crawl.txt' and 'documents/last-crawl.txt' are the same file alifathan@alifathan-VirtualBox: $ find. -type f -name "*.jpg" -exec mv {} images/ \; 
mv: './images/i1.jpg' and 'images/i1.jpg' are the same file 
mv: './images/i2.jpg' and 'images/i2.jpg' are the same file 
mv: './images/i3.jpg' and 'images/i3.jpg' are the same file 
mv: './images/i4.jpg' and 'images/i4.jpg' are the same file 
mv: './images/i5.jpg' and 'images/i5.jpg' are the same file
alifathan@alifathan-VirtualBox: $ find . -type f -name "*.zip" -exec cp {} achives/ \;
cp: './achives/a5.zip' and 'achives/a5.zip' are the same file 
cp: './achives/a3.zip' and 'achives/a3.zip' are the same file 
cp: './achives/a4.zip' and 'achives/a4.zip' are the same file 
cp: './achives/a2.zip' and 'achives/a2.zip' are the same file 
cp: './achives/a1.zip' and 'achives/a1.zip' are the same file
alifathan@alifathan-VirtualBox: $ find . -type f -name "*.log" -exec mv {} logs/ \; 
mv: './logs/12.log' and 'logs/12.log' are the same file 
mv: './logs/l1.log' and 'logs/l1.log' are the same file
